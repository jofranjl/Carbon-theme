        "tokenColors": [
            {
                "scope": [
                    "variable",
                    "variable.other.readwrite",
                    "variable.other.property",
                    "variable.other.object.property",
                    "variable.parameter",
                    "meta.definition.variable.name",
                    "meta.object-literal.key",
                    "meta.class.body entity.name.variable"
                ],
                "settings": {
                    "foreground": "#585858"
                }
            },
            {
                "scope": [
                    "meta.object.member",
                    "support.type.property-name",
                    "meta.property.object",
                    "meta.class.body entity.name.function"
                ],
                "settings": {
                    "foreground": "#0ea5e9"
                }
            },
            {
                "scope": [
                    "constant",
                    "constant.numeric",
                    "constant.language",
                    "constant.character.escape",
                    "constant.other.color",
                    "meta.number.integer.decimal",
                    "meta.number.float.decimal"
                ],
                "settings": {
                    "foreground": "#1d4ed8"
                }
            },
            {
                "scope": [
                    "string",
                    "string.quoted.double",
                    "string.quoted.single",
                    "string.template"
                ],
                "settings": {
                    "foreground": "#4bb8b8"
                }
            },
            {
                "scope": [
                    "keyword",
                    "keyword.control",
                    "keyword.operator.new",
                    "keyword.operator"
                ],
                "settings": {
                    "foreground": "#a855f7"
                }
            },
            {
                "scope": [
                    "entity.name.type.class",
                    "entity.name.type.interface",
                    "support.class",
                    "support.type",
                    "entity.name.type.enum",
                    "entity.name.type.namespace",
                    "entity.name.type"
                ],
                "settings": {
                    "foreground": "#ff00bf"
                }
            },
            {
                "scope": [
                    "entity.name.function",
                    "support.function",
                    "meta.function-call"
                ],
                "settings": {
                    "foreground": "#ffffff"
                }
            },
            {
                "scope": [
                    "comment",
                    "punctuation.definition.comment"
                ],
                "settings": {
                    "foreground": "#546781",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": [
                    "punctuation",
                    "punctuation.separator",
                    "punctuation.definition.string",
                    "punctuation.definition.comment",
                    "punctuation.definition.variable",
                    "punctuation.definition.brace",
                    "punctuation.definition.tag"
                ],
                "settings": {
                    "foreground": "#22d3ee"
                }
            },
            {
                "scope": [
                    "source.js",
                    "source.jsx",
                    "source.ts",
                    "source.tsx",
                    "meta.function-call.js",
                    "meta.function-call.jsx",
                    "meta.function-call.ts",
                    "meta.function-call.tsx"
                ],
                "settings": {
                    "foreground": "#38bdf8"
                }
            },
            {
                "scope": [
                    "keyword.control.robot",
                    "meta.keyword.robot",
                    "entity.name.function.robot"
                ],
                "settings": {
                    "foreground": "#af38ff"
                }
            },
            {
                "scope": [
                     "meta.function-call.robot",
                "entity.other.keyword.robot"
                ],
                "settings": {
                    "foreground": "#f1f1f1"
                },
            }

        ]
}
