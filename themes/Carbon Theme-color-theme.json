    "$schema": "vscode://schemas/color-theme",
    "name": "Carbon Theme",
    "author": "Jofran Lima",
    "maintainers": [
        "Jofran Lima <jofran.alves@live.com>"
    ],
    
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "annotation:dart": "#BD93F9",
        "class:dart": "#58747a",
        "property:dart": "#BD93F9",
     // Personalização para strings interpoladas (f-strings) no Python
    "string.interpolated.python": {
        "foreground": "#00ff6a", // Verde brilhante
        "fontStyle": "" // Estilo padrão
      },
  
      // Personalização para variáveis dentro de f-strings
      "variable.other.readwrite.python": {
        "foreground": "#ff0077", // Rosa brilhante
        "fontStyle": "" // Estilo padrão
      },
  
      // Personalização para funções no Python
      "function:python": {
        "foreground": "#50FA7B", // Verde
        "fontStyle": "" // Estilo padrão
      },
  
      // Personalização para variáveis no Python
      "variable:python": {
        "foreground": "#adfdd8", // Laranja
        "fontStyle": "" // Estilo padrão
      },
        // Personalização para palavras-chave no JavaScript
        "keyword:javascript": {
          "foreground": "#FF4500", // Vermelho-alaranjado
          "fontStyle": "" // Estilo padrão
        },
        // Personalização para classes no JavaScript
        "class:javascript": {
          "foreground": "#58747a", // Azul-cinza
          "fontStyle": "" // Estilo padrão
        },
        // Personalização para strings interpoladas no JavaScript (template literals)
        "string.interpolated.javascript": {
          "foreground": "#00FF6A", // Verde brilhante
          "fontStyle": "" // Estilo padrão
        },
        // Personalização para variáveis dentro de template literals
        "variable.other.readwrite.javascript": {
          "foreground": "#FF0077", // Rosa brilhante
          "fontStyle": "" // Estilo padrão
        },
        // Personalização para funções no JavaScript
        "function:javascript": {
          "foreground": "#50FA7B", // Verde
          "fontStyle": "" // Estilo padrão
        },
        // Personalização para variáveis no JavaScript
        "variable:javascript": {
          "foreground": "#adfdd8", // Laranja claro
          "fontStyle": "" // Estilo padrão
        },
        "punctuation.definition.string": {
      "foreground": "#FF4500",
        },
        // Personalização para propriedades em objetos JavaScript
        "property:javascript": {
          "foreground": "#BD93F9", // Roxo claro
          "fontStyle": "" // Estilo padrão
        },
        
        
      },
